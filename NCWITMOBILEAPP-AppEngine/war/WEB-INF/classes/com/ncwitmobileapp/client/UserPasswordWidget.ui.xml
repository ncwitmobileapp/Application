<!-- UserPasswordWidget.ui.xml -->

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:c='urn:import:com.google.gwt.cell.client' xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:t='urn:import:com.ncwitmobileapp.client' xmlns:d='urn:import:com.google.gwt.user.datepicker.client' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel>
		<div id="container">
			<div id="android"></div>
			<div class="appname">Logins and Passwords<div class="balls"></div></div>
			<div class="roundrect">
				<div class="inputcell">
					<g:TextBox ui:field="userPasswordInput" />
				</div>
				<div class="userpasswordtable">
					<t:UserPasswordWidget.UserPasswordTable
						ui:field="userPasswordTable" tableLayoutFixed="true" />
				</div>
				<div class="user"><g:Label ui:field="userpasswordsignin"/></div>
			</div>
		</div>
	</g:HTMLPanel>

</ui:UiBinder>
